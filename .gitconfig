[user]
  name = Jan Pieper
  email = kontakt@jan-pieper.info
[alias]
  unstage = reset HEAD --
  who = !git shortlog --summary --email --numbered
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
[core]
  excludesfile = ~/.gitexcludes
  editor = vi
[push]
  default = upstream
[branch]
  autosetupmerge = true
  autosetuprebase = always
[merge]
  summary = true
[rebase]
  stat = true
[url "git@github.com:"]
  insteadOf = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
  insteadOf = "github:"
[url "git@gist.github.com:"]
  insteadOf = "gst:"
  pushInsteadOf = "gist:"
  pushInsteadOf = "git://gist.github.com"
[url "git://gist.github.com/"]
  insteadOf = "gist:"
[github]
  user = janpieper
